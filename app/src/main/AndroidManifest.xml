<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.klz.reacnathuris">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:name=".LoginActivity"
            android:label="@string/login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/registrar" />
        <activity
            android:name=".MenuGeralActivity"
            android:label="@string/menu" />
        <activity
            android:name=".MenuCadastrosActivity"
            android:label="@string/cadastros" />
        <activity
            android:name=".CadPacientesActivity"
            android:label="@string/pacientes" />
        <activity
            android:name=".CadComplementoPacientesActivity"
            android:label="@string/pacientes" />
        <activity
            android:name=".CadUnidadesActivity"
            android:label="@string/unidades" />
        <activity
            android:name=".CadProtocolosActivity"
            android:label="@string/protocolos" />
        <activity
            android:name=".CalendarActivity"
            android:label="@string/agenda">
        </activity>
        <activity
            android:name=".AgendarAtendimentoActivity"
            android:label="@string/agenda">
        </activity>
    </application>

</manifest>